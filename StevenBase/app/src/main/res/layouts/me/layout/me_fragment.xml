<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hx.stevenbase.ui.Set.me.MeFragment"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    android:background="@color/white">

    <com.hx.steven.component.BannerView.BannerView
        android:id="@+id/bannerView_me"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_175">
    </com.hx.steven.component.BannerView.BannerView>
    <com.hx.steven.component.SubmitButton
        android:id="@+id/sbBtn_login"
        android:layout_marginTop="50dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </com.hx.steven.component.SubmitButton>
</LinearLayout>
